#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Volodymyr/Desktop/Pet_Projects/_Zwiach/_Frontend_Mentor/equalizer-landing-page/equalizer-landing-page/starter-code/node_modules/.pnpm/tailwindcss@3.4.17/node_modules/tailwindcss/lib/node_modules:/mnt/c/Users/Volodymyr/Desktop/Pet_Projects/_Zwiach/_Frontend_Mentor/equalizer-landing-page/equalizer-landing-page/starter-code/node_modules/.pnpm/tailwindcss@3.4.17/node_modules/tailwindcss/node_modules:/mnt/c/Users/Volodymyr/Desktop/Pet_Projects/_Zwiach/_Frontend_Mentor/equalizer-landing-page/equalizer-landing-page/starter-code/node_modules/.pnpm/tailwindcss@3.4.17/node_modules:/mnt/c/Users/Volodymyr/Desktop/Pet_Projects/_Zwiach/_Frontend_Mentor/equalizer-landing-page/equalizer-landing-page/starter-code/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Volodymyr/Desktop/Pet_Projects/_Zwiach/_Frontend_Mentor/equalizer-landing-page/equalizer-landing-page/starter-code/node_modules/.pnpm/tailwindcss@3.4.17/node_modules/tailwindcss/lib/node_modules:/mnt/c/Users/Volodymyr/Desktop/Pet_Projects/_Zwiach/_Frontend_Mentor/equalizer-landing-page/equalizer-landing-page/starter-code/node_modules/.pnpm/tailwindcss@3.4.17/node_modules/tailwindcss/node_modules:/mnt/c/Users/Volodymyr/Desktop/Pet_Projects/_Zwiach/_Frontend_Mentor/equalizer-landing-page/equalizer-landing-page/starter-code/node_modules/.pnpm/tailwindcss@3.4.17/node_modules:/mnt/c/Users/Volodymyr/Desktop/Pet_Projects/_Zwiach/_Frontend_Mentor/equalizer-landing-page/equalizer-landing-page/starter-code/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli.js" "$@"
else
  exec node  "$basedir/../../lib/cli.js" "$@"
fi
